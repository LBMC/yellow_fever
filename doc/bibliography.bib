  author = {Buttner, Maren and Miao, Zhichao and Wolf, Alexander and Teichmann, Sarah A and Theis, Fabian J},
  month = oct,
  year = {2017},
  file = {/Users/laurent/Zotero/storage/LV6PMYS9/Buttner et al. - 2017 - Assessment of batch-correction methods for scRNA-s.pdf}
}


@article{negriFeminizingWolbachiaZyginidia2006a,
  title = {Feminizing {{Wolbachia}} in {{Zyginidia}} Pullula ({{Insecta}}, {{Hemiptera}}), a Leafhopper with an {{XX}}/{{X0}} Sex-Determination System},

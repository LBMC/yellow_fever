@article{hoffBRAKER1UnsupervisedRNASeqBased2016a,
  title = {{{BRAKER1}}: {{Unsupervised RNA}}-{{Seq}}-{{Based Genome Annotation}} with {{GeneMark}}-{{ET}} and {{AUGUSTUS}}},
  volume = {32},
